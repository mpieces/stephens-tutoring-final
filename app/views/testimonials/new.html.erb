<h1>New Testimonial</h1>

<%= form_for(@testimonial) do |f| %>


  <div class="form-group">
    <%= f.text_area :feedback, rows: 15, :placeholder => "Feedback...." %><br>
  </div>
  <div class="form-group">
    <%= f.text_field :name, :placeholder => "Name (optional)" %><br>
  </div>
  <div class="actions">
    <%= f.submit 'Submit Feedback' %>
  </div>
<% end %>
<br>
<p><%= link_to 'Back', testimonials_path %></p>
